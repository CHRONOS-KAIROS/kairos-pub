            },
            {
              "slotName": "targetLocation",
              "role": "kairos:Recipient",
              "constraints": [
                "kairos:LOC",
                "kairos:FAC"
              ]
            }
          ],
          "alias": [
            "kairos:nonsensical",
            "sbu:sbunonsense",
            "wordnet:00608800"
