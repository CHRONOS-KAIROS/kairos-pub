          "before": [
            "isi:myevent_1",
            "isi:myevent_2"
          ],
          "after": [
            "isi:myevent_3",
            "isi:myevent_4"
          ]
        },
        {
          "overlaps": [
            "isi:myevent_5",
            "isi:myevent_6"
          ]
        },
        {
          "container": "isi:myevent_2",
          "contained": [
            "isi:myevent_3",
            "isi:myevent_4"
          ],
          "flags": [
            "optional",
            "causes",
            "precondition",
            "simultaneous",
            "subevent"
          ]
        }
