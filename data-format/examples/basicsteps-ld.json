        },
        {
          "@type": "isi:mystep",
          "@id": "isi:myevent_2"
        },
        {
          "@type": "isi:mystep",
          "@id": "isi:myevent_3"
        },
        {
          "@type": "isi:mystep",
          "@id": "isi:myevent_4"
        },
        {
          "@type": "isi:mystep",
          "@id": "isi:myevent_5"
