    "super": "kairos:super",
    "name": "kairos:name",
    "description": "kairos:description",
    "version": "kairos:version",
    "steps": "kairos:steps",
    "events": "kairos:events",
    "ref": "kairos:ref",
    "slots": "kairos:slots",
    "slotName": "kairos:slotName",
    "role": "kairos:role",
    "constraint": "kairos:constraint",
